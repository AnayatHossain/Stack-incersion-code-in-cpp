//Incersion
#include <iostream>
using namespace std;

int main() {
    int stack[100]; // Array to store stack elements
    int top = -1; // Initialize top of stack to -1

    // Push elements onto stack
    stack[++top] = 1;
    stack[++top] = 2;
    stack[++top] = 3;
    stack[++top] = 4;
    stack[++top] = 5;

    // Print stack elements
    for (int i = 0; i <= top; i++) {
        cout << stack[i] << " ";
    }
    cout << endl;

    return 0;
}

// Delation

#include <iostream>
using namespace std;

int main() {
    int stack[100]; // Array to store stack elements
    int top = -1; // Initialize top of stack to -1

    // Push elements onto stack
    stack[++top] = 1;
    stack[++top] = 2;
    stack[++top] = 3;
    stack[++top] = 4;
    stack[++top] = 5;

    // Pop elements from stack
    while (top >= 0) {
        cout << "Popped element: " << stack[top--] << endl;
    }

    return 0;
}
#include <iostream>
using namespace std;

int main() {
    int stack[100]; // Array to store stack elements
    int top = -1; // Initialize top of stack to -1

    // Push elements onto stack
    stack[++top] = 1;
    stack[++top] = 2;
    stack[++top] = 3;
    stack[++top] = 4;
    stack[++top] = 5;

    // Pop elements from stack
    while (top >= 0) {
        cout << "Popped element: " << stack[top--] << endl;
    }

    return 0;
}
